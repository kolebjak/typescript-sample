{
  "name": "ts-project-sample",
  "version": "1.0.0",
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "history": "4.7.2",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
